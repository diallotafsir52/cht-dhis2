{
  "trackedEntity": "<patient_id>", // Unique identifier for the tracked entity, typically patient ID.
  "orgUnit": "facility_id", // Organization Unit or Health Facility ID from CHT.
  "trackedEntityType": "<tracked_entity_type>", // The type of entity being tracked, such as patient.
  "attributes": [
    {
      "attribute": "field_id_1", // Attribute ID in DHIS2, mapped from CHT field ID.
      "value": "field_value_1" // Collected value for the specific attribute.
    },
    {
      "attribute": "field_id_2",
      "value": "field_value_2"
    }
    // Add additional attributes as needed.
  ],
  "enrollments": [
    {
      "enrollment": "<enrollment_id>", // Unique ID for the enrollment, generated by DHIS2.
      "orgUnit": "facility_id", // Facility where enrollment took place.
      "program": "event_type", // DHIS2 program ID, mapped from CHT's event type.
      "enrolledAt": "registration_date", // Date of enrollment in `YYYY-MM-DDTHH:MM:SS.SSS` format.
      "incidentDate": "incident_date", // Date of the incident or first report, same format.
      "status": "form_status", // Status of the enrollment (e.g., ACTIVE).
      "events": [
        {
          "event": "<event_id>", // Unique identifier for the event.
          "programStage": "visit_type", // Stage of the program, mapped to DHIS2 programStage ID.
          "orgUnit": "facility_id", // Facility where the event occurred.
          "occurredAt": "visit_date", // Date the event took place in `YYYY-MM-DDTHH:MM:SS.SSS` format.
          "scheduledAt": "scheduled_date", // If applicable, the scheduled date for the event.
          "status": "event_status", // Status of the event (e.g., ACTIVE, COMPLETED).
          "dataValues": [
            {
              "dataElement": "data_point_id_1", // Data Element ID from DHIS2.
              "value": "data_point_value_1" // Collected value for the data element.
            },
            {
              "dataElement": "data_point_id_2",
              "value": "data_point_value_2"
            }
            // Add additional data elements as needed.
          ],
          "notes": [
            {
              "value": "notes", // Any comments or notes about the event from CHT.
              "storedBy": "CHT_user", // Optional: Name of the user who added the note.
              "storedDate": "YYYY-MM-DDTHH:MM:SS.SSS" // Optional: Date when the note was added.
            }
          ],
          "coordinate": {
            "latitude": "latitude", // If available, latitude of the event's location.
            "longitude": "longitude" // If available, longitude of the event's location.
          }
        }
        // Add additional events as needed.
      ]
    }
  ]
}
